<div class="card">
  <div class="card-header d-flex align-items-center justify-content-between">
    Home Owner Stat

    <a data-toggle="collapse" href="#user_home_owner_stat_new_form">
      Add relationship
    </a>
  </div>

  <ul class="list-group list-group-flush">
    <% @user.home_owner_stat.each do |relationship| %>
      <li class="list-group-item d-flex align-items-center justify-content-between">
        <!-- Show link start -->
        <a href="/relationships/<%= relationship.id %>">
          <%= relationship.home_owner_id %>
        </a>
        <!-- Show link end -->

      </li>
    <% end %>

  </ul>
</div>
